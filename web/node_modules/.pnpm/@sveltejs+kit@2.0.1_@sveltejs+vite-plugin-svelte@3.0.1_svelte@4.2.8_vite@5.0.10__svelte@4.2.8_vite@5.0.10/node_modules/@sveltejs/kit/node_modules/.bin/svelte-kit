#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/devnull03/static-svelte-site-template/web/node_modules/.pnpm/@sveltejs+kit@2.0.1_@sveltejs+vite-plugin-svelte@3.0.1_svelte@4.2.8_vite@5.0.10__svelte@4.2.8_vite@5.0.10/node_modules/@sveltejs/kit/node_modules:/home/devnull03/static-svelte-site-template/web/node_modules/.pnpm/@sveltejs+kit@2.0.1_@sveltejs+vite-plugin-svelte@3.0.1_svelte@4.2.8_vite@5.0.10__svelte@4.2.8_vite@5.0.10/node_modules/@sveltejs/node_modules:/home/devnull03/static-svelte-site-template/web/node_modules/.pnpm/@sveltejs+kit@2.0.1_@sveltejs+vite-plugin-svelte@3.0.1_svelte@4.2.8_vite@5.0.10__svelte@4.2.8_vite@5.0.10/node_modules:/home/devnull03/static-svelte-site-template/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/devnull03/static-svelte-site-template/web/node_modules/.pnpm/@sveltejs+kit@2.0.1_@sveltejs+vite-plugin-svelte@3.0.1_svelte@4.2.8_vite@5.0.10__svelte@4.2.8_vite@5.0.10/node_modules/@sveltejs/kit/node_modules:/home/devnull03/static-svelte-site-template/web/node_modules/.pnpm/@sveltejs+kit@2.0.1_@sveltejs+vite-plugin-svelte@3.0.1_svelte@4.2.8_vite@5.0.10__svelte@4.2.8_vite@5.0.10/node_modules/@sveltejs/node_modules:/home/devnull03/static-svelte-site-template/web/node_modules/.pnpm/@sveltejs+kit@2.0.1_@sveltejs+vite-plugin-svelte@3.0.1_svelte@4.2.8_vite@5.0.10__svelte@4.2.8_vite@5.0.10/node_modules:/home/devnull03/static-svelte-site-template/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../svelte-kit.js" "$@"
else
  exec node  "$basedir/../../svelte-kit.js" "$@"
fi
