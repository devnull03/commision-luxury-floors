#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/devnull03/static-svelte-site-template/web/node_modules/.pnpm/svelte-check@3.6.2_postcss-load-config@4.0.2_postcss@8.4.32__postcss@8.4.32_svelte@4.2.8/node_modules/svelte-check/bin/node_modules:/home/devnull03/static-svelte-site-template/web/node_modules/.pnpm/svelte-check@3.6.2_postcss-load-config@4.0.2_postcss@8.4.32__postcss@8.4.32_svelte@4.2.8/node_modules/svelte-check/node_modules:/home/devnull03/static-svelte-site-template/web/node_modules/.pnpm/svelte-check@3.6.2_postcss-load-config@4.0.2_postcss@8.4.32__postcss@8.4.32_svelte@4.2.8/node_modules:/home/devnull03/static-svelte-site-template/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/devnull03/static-svelte-site-template/web/node_modules/.pnpm/svelte-check@3.6.2_postcss-load-config@4.0.2_postcss@8.4.32__postcss@8.4.32_svelte@4.2.8/node_modules/svelte-check/bin/node_modules:/home/devnull03/static-svelte-site-template/web/node_modules/.pnpm/svelte-check@3.6.2_postcss-load-config@4.0.2_postcss@8.4.32__postcss@8.4.32_svelte@4.2.8/node_modules/svelte-check/node_modules:/home/devnull03/static-svelte-site-template/web/node_modules/.pnpm/svelte-check@3.6.2_postcss-load-config@4.0.2_postcss@8.4.32__postcss@8.4.32_svelte@4.2.8/node_modules:/home/devnull03/static-svelte-site-template/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/svelte-check" "$@"
else
  exec node  "$basedir/../../bin/svelte-check" "$@"
fi
